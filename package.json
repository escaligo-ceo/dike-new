{
  "name": "dike",
  "version": "0.6.382",
  "description": "Piattaforma gestionale per Avvocati e Studi Legali",
  "author": "Tommaso Centurioni",
  "private": true,
  "license": "UNLICENSED",
  "type": "commonjs",
  "engines": {
    "node": ">=22.14.0 <23",
    "npm": "11.4.1",
    "yarn": "1.22.22"
  },
  "scripts": {
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "clean": "rm -rf .yarn coverage node_modules tsconfig.build.tsbuildinfo package-lock.json",
    "migrate": "npx sequelize-cli db:migrate",
    "db:stop": "docker compose down postgres",
    "swagger": "ts-node scripts/swagger",
    "docker-compose:reset": "docker compose down -v",
    "docker-compose:up:d": "docker compose up -d",
    "docker-compose:build": "docker compose build",
    "docker-compose:build:all": "docker compose build",
    "docker-compose:start": "docker compose up",
    "docker-compose:start-d": "docker compose up --build -d postgres keycloak",
    "docker-compose:rebuild": "docker compose build --no-cache",
    "docker-compose:stop": "docker compose stop",
    "release:major": "node ./scripts/release.js major $(pwd)",
    "release:minor": "node ./scripts/release.js minor $(pwd)",
    "release:patch": "node ./scripts/release.js patch $(pwd)",
    "release": "node ./scripts/release.js patch $(pwd)",
    "release:custom": "node ./scripts/releaseCustom.js $(pwd)",
    "migration:generate": "node scripts/generate-migration.js",
    "seed:generate": "node scripts/generate-seed.js",
    "service-start": "yarn clean && yarn && yarn docker-compose:start-d",
    "cache:clean": "rm -rf node_modules yarn.lock package-lock.json && yarn install",
    "r": "yarn service-start",
    "l": "yarn docker-compose:start-d",
    "b": "yarn docker-compose:rebuild && yarn docker-compose:up:d"
  },
